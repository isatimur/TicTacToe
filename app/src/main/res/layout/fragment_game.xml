<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/ru.isatimur.ttt.tictactoe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".GameFragment"
    tools:ignore="ResAuto">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ttt_row1"
        android:layout_weight="1">

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell0"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell1"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell2"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#ff000000" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ttt_row2"
        android:layout_weight="1">

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell3"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell4"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell5"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#ff000000" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:id="@+id/ttt_row3">

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell6"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell7"
            android:layout_weight="1" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#ff000000"></LinearLayout>

        <ru.isatimur.ttt.tictactoe.CellView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/ttt_cell8"
            android:layout_weight="1" />
    </LinearLayout>

</LinearLayout>
